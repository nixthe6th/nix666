# Polymarket Bot Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to Git!

# Required: Your wallet private key (from MetaMask/Phantom)
# Format: 0x1234...
POLYMARKET_PRIVATE_KEY=

# Optional: Set to 'false' for live trading
# Default: 'true' (dry run mode - no real trades)
DRY_RUN=true
